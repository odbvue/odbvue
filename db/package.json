{
  "name": "@odbvue/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "pnpm -w exec tsc -p db/tsconfig.json",
    "type-check": "pnpm -w exec tsc -p db/tsconfig.json",
    "lint": "pnpm -w exec eslint db --cache",
    "lint:fix": "pnpm -w exec eslint db --fix --cache",
    "format": "pnpm -w exec prettier -w db",
    "format:check": "pnpm -w exec prettier -c db"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@types/oracledb": "^6.10.0",
    "@types/prompts": "^2.4.9",
    "@types/unzipper": "^0.10.11"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "dotenv": "^16.4.5",
    "oracledb": "^6.5.0",
    "prompts": "^2.4.2",
    "unzipper": "^0.12.3"
  }
}
