{
  "name": "cli",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -w exec tsc -p cli/tsconfig.json",
    "type-check": "pnpm -w exec tsc -p cli/tsconfig.json",
    "lint": "pnpm -w exec eslint cli --cache",
    "lint:fix": "pnpm -w exec eslint cli --fix --cache",
    "format": "pnpm -w exec prettier -w cli",
    "format:check": "pnpm -w exec prettier -c cli"
  },
  "devDependencies": {
    "@types/node": "^24.10.4",
    "@types/oracledb": "^6.10.0",
    "@types/prompts": "^2.4.9",
    "@types/unzipper": "^0.10.11"
  },
  "dependencies": {
    "chalk": "^5.6.2",
    "dotenv": "^16.4.5",
    "oracledb": "^6.5.0",
    "prompts": "^2.4.2",
    "unzipper": "^0.12.3"
  }
}
